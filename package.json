{
  "name": "hairstyle-1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "analyze-images": "npm run optimize-images && echo 'Image analysis complete'",
    "analyze": "ANALYZE=true npm run build",
    "perf-audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "build:analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "lcp-test": "npm run build && npm run dev & sleep 15 && echo 'Testing LCP optimizations...' && curl -I http://localhost:3000",
    "test:idempotency": "node test-idempotency.js",
    "test:idempotency:quick": "node test-idempotency.js --repeat 3 --concurrent 2",
    "test:idempotency:stress": "node test-idempotency.js --repeat 20 --concurrent 10",
    "test:idempotency:prod": "TEST_BASE_URL=https://hair-style.ai node test-idempotency.js",
    "db:migrate": "node scripts/run-migration.js",
    "pages:build": "npx @cloudflare/next-on-pages@latest",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.7",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "daisyui": "^5.0.54",
    "drizzle-orm": "^0.44.5",
    "google-auth-library": "^10.3.0",
    "http-browserify": "^1.7.0",
    "http-status-codes": "^2.3.0",
    "https-browserify": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.522.0",
    "next": "15.5.2",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-before-after-slider-component": "^1.1.8",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.33.5",
    "simple-flakeid": "^0.0.5",
    "stripe": "^18.5.0",
    "swr": "^2.3.6",
    "tailwind-scrollbar": "^3.1.0",
    "undici": "^7.15.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.15",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "npmRegistryServer": "https://registry.npmmirror.com"
}
